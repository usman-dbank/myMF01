{
  "jsc": {
    "transform": {
        "legacyDecorator": true,
        "decoratorMetadata": true,
        "react": {
          "runtime": "automatic",
          "throwIfNamespace": true,
          "useBuiltins": true,
          "development": false
        }
      },
      "loose": true,
      "parser": {
        "syntax": "typescript",
        "tsx": true,
        "decorators": true
      },
      "target": "es5",
      "minify": {
        "compress": false,
        "mangle": false
      }
  },
  "module": {
    "type": "es6"
  },
  "isModule": true,
  "minify": true
}