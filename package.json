  "scripts": {
    "pre-install": "npm config -L project set //din-global-187429986583.d.codeartifact.us-east-1.amazonaws.com/npm/din.global.tools/:_authToken=$(aws codeartifact get-authorization-token --domain din-global --domain-owner 187429986583 --query authorizationToken --output text)",
    "build": "webpack --config webpack/webpack.config.js --env env=prod",
    "start": "webpack serve --config webpack/webpack.config.js --env env=dev",
    "build:inject-env": "cp sample.env .env && npx react-inject-env set -d ./dist/microfe-template",
    "test": "jest --coverage --config=jest.config.ts",
    "test:ci": "jest --watchAll=false --ci --coverage --forceExit --detectOpenHandles --reporters=default --reporters=@casualbot/jest-sonar-reporter  --coverageReporters=cobertura  --coverageReporters=lcov --config=jest.config.ts",
    "eject": "react-scripts eject",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prepare": "husky install",
    "codegen": "REACT_APP_GRAPHQL_URL=http://localhost:3000/graphql graphql-codegen --config codegen.ts",
    "test-e2e:ci": "CI=true npx playwright test",
    "test-e2e": "npx playwright test",
    "test-e2e:update-snapshot": "npx playwright test --update-snapshots",
    "test-e2e:show-report": "npx playwright show-report",
    "test-e2e:codegen": "npx playwright codegen",
    "test-e2e:run": "sh run-e2e.sh",
    "test-e2e:run-update-snapshot": "sh run-e2e.sh update-snapshot"
  },
